        </div>
    </div>
</aside>

<main class="flex-1 ml-64 overflow-y-scroll max-h-screen">
    <div class="pt-12 pl-9 flex">
        <div>
            <h1 class="text-display_sm font-medium">Tambah Siswa</h1>
            <p class="text-text_md font-regular text-gray-500">Atur kelas dan siswa untuk setiap tingkatan</p>
        </div>
    </div>
    
    <div class="flex items-start pl-9 pt-5">
        <div class="p-5 w-72  mr-14 border border-gray-200 shadow-md rounded-lg">
            <div class="pb-5">
                <h1 class="text-text_lg font-medium">Jenis Studi</h1>
                <p class="text-text_md font-regular">Reguler</p>
            </div>
            <div class="pb-5"> 
                <h1 class="text-text_lg font-medium">Tingkatan</h1>
                <p class="text-text_md font-regular"><%= dataKelas.jurusan.tingkatan %></p>
            </div>
            <div class="pb-5">
                <h1 class="text-text_lg font-medium">Nama Kelas</h1>
                <p class="text-text_md font-regular"><%= dataKelas.nama %></p>
            </div>
            <div class="pb-5">
                <h1 class="text-text_lg font-medium">Jurusan</h1>
                <p class="text-text_md font-regular"><%= dataKelas.jurusan.jurusan %></p>
            </div>
            <div class="pb-5">
                <h1 class="text-text_lg font-medium">Standar Kurikulum</h1>
                <p class="text-text_md font-regular"><%= dataKelas.jurusan.standarKurikulum %></p>
            </div>
        </div>
        
        <div class="pr-14 pb-10 w-full">
            <div class="overflow-x-auto w-full shadow-md sm:rounded-lg">
                <table class="w-full text-left text-gray-500">
                    <caption class="p-5 font-semibold text-text-base bg-white">
                        <div class="flex items-center">
                            <h1 class="text-left flex-1 text-display_xs">Daftar Siswa</h1>
                            <div>
                                <a href="/siswa/<%= admin._id %>/tambah_siswa/<%= dataKelas._id %>">
                                    <button type="button" class="bg-primary-600 text-white text-text_sm rounded-lg text-center inline-flex items-center px-4 py-3">
                                        <span class="mr-2"><img src="/assets/plus.png" alt="" class=" w-5 h-5"></span>
                                        Tambah Siswa</button>
                                </a>
                            </div>
                        </div>
                    </caption>
                    <thead class="text-text_sm font-medium text-gray-500 bg-gray-50">
                        <tr>
                            <th scope="col" class="py-3 px-6">
                                Nama Siswa
                            </th>
                            <th scope="col" class="py-3 px-6">
                                NISN
                            </th>
                            <th scope="col" class="py-3 px-6">
                                Date Uploaded
                            </th>
                            <th scope="col" class="py-3 px-6">
                                Last Updated
                            </th>
                            <th scope="col" class="py-3 px-6">
                                <span class="sr-only"></span>
                            </th>
                            <th scope="col" class="py-3 px-6">
                                <span class="sr-only"></span>
                            </th>
                            <th scope="col" class="py-3 px-6">
                                <span class="sr-only"></span>
                            </th>
                        </tr>
                    </thead>
                    <div >
                    <tbody  id="abc"  >
                        <div>
                       
                        <% dataKelas.siswa.forEach(siswa => { %>
                            <tr class="bg-white border-b">
                                <th scope="row" class="py-7 px-8 font-medium text-text-base whitespace-nowrap">
                                    <%= siswa.namaSiswa %>
                                </th>
                                <td class="py-7 px-8 ">
                                    <%= siswa.nisn %>
                                </td>
                                <td class="py-7 px-8 ">
                                    <%= moment(siswa.createdAt).format("lll") %>
                                </td>
                                <td class="py-7 px-8 ">
                                    <%= moment(siswa.updatedAt).format("lll") %>
                                </td>
                                <td class="py-7 px-8 scroll-px-1">
                                    <a href="/siswa/<%= admin._id %>/<%= dataKelas._id %>/<%= siswa._id %>">
                                        <img src="/assets/plus-black.png" alt="" style="height:20px ; width: 20px;">
                                    </a>
                                </td><td class="py-7 px-1">
                                    <a href="/siswa/<%= admin._id %>/<%= dataKelas._id %>/<%= siswa._id %>/edit">
                                        <img src="/assets/edit-2.png" alt="" style="height:20px ; width: 20px;">
                                    </a>
                                </td>
                                <td class="py-7 pl-2 pr-4">
                                    <form action="/siswa/hapusSiswa?_method=DELETE" method="POST">
                                        <input type="hidden" name="siswaId" value="<%= siswa._id %>">
                                        <input type="hidden" name="adminId" value="<%= admin._id %>">
                                        <input type="hidden" name="kelasId" value="<%= dataKelas._id %>">
                                        <button type="submit" onclick="return confirm('Yakin ingin menghapus siswa?')"><img src="/assets/trash.png" alt="" style="height:20px; width: 20px;"></button>
                                    </form>
                                </td>
                            </tr>
                        <% }) %>
                    </div>
                    </tbody>
                </div>
                </table>
            </div>
        </div>
    </div>
</main>

